<div class="project-item col-sm-3 col-lg-3 col-md-3">
  <div class="thumbnail">
    <img src="<%= art.escape("image_file_name") %>"alt="">
  </div>
  <div class="project-content">
    <div class="project-caption">
      <h4>Title: <a href="#arts/<%=art.id%>"><%=art.escape("title")%></a></h4>
      <div class="project-description">
        <p>Description: <%=art.escape("description")%></h6></p>
      </div>
      <h6><a href="#users/<%=art.escape("artist_id")%>">Go to this Artist's Page</a></h6>
    </div>
  </div>
</div>
</div>
