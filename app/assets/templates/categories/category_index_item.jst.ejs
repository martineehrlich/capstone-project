<% if(category.id === 1){ %>
<div class="category category-item active">
  <a class="category-item" data-id="<%=category.id%>"><%= category.escape("name") %></a>
</div>
<% }else { %>
<div class="category category-item">
  <a class="category-item" data-id="<%=category.id%>"><%= category.escape("name") %></a>
</div>
<% } %>
